<view><cu-custom vue-id="1" bgColor="bg-gradual-orange" bind:__l="__l" vue-slots="{{['default','content']}}">><view slot="content">个人中心</view></cu-custom><view class="userinfo" style="margin-bottom:50rpx;"><image class="userinfo-avatar" src="{{userInfo.avatarUrl?userInfo.avatarUrl:'../../static/images/user.png'}}" background-size="cover"></image><block wx:if="{{!userInfo.nickName}}"><button open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['login',['$event']]]]]}}" bindgetuserinfo="__e">点击登录账户</button></block><block wx:else><text class="userinfo-nickname">{{userInfo.nickName}}</text></block></view><picker class="picker" range="{{userIDList}}" value="{{selectedIndex}}" data-event-opts="{{[['change',[['choose',['$event']]]]]}}" bindchange="__e"><view class="cell _div"><view class="choose _div">用户</view><view class="_div">{{''+userIDList[selectedIndex]+''}}<i-icon vue-id="2" type="enter" bind:__l="__l"></i-icon></view></view></picker><button class="login-button" hover-class="clicked" loading="{{loading}}" data-event-opts="{{[['tap',[['handleLogin',['$event']]]]]}}" bindtap="__e">登录</button></view>